{

  "description": "Save visited tweets to Azure cloud",
  "manifest_version": 2,
  "name": "TweetsFullTextSearch",
  "version": "1.0",
  "homepage_url": "https://github.com",
  "icons": {
    "48": "icons/border-48.png"
  },
  "permissions": [
    "https://*.azurewebsites.net/*",
    "storage"
  ],

  "content_scripts": [  
    {
      "matches": ["*://*.twitter.com/*"],
      "js": ["script.js"]
    }
  ]

}
